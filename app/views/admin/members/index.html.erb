<h3>会員一覧</h3>
<table>
	<thead>
		<tr>
			<th>会員ID</th>
			<th>氏名</th>
			<th>メールアドレス</th>
			<th>ステータス</th>
		</tr>
	</thead>
	<% @members.each do |member| %>
	<tr>
		<td><%= member.id %></td>
		<td>
			<%= link_to admin_member_path(member.id) do %>
			<%= member.first_name member.last_name %>
			<% end %>
			</td>
		<td><%= member.email %></td>
		<td><%= member.status %></td>
	</tr>
	<% end %>
</table>
